{
  "name": "@folio/erm-usage",
  "version": "0.1.0",
  "description": "eUsage Module",
  "main": "src/index.js",
  "repository": "",
  "license": "Apache-2.0",
  "engines": {
    "node": ">=6.0.0"
  },
  "scripts": {
    "start": "stripes serve",
    "build": "stripes build --output ./output",
    "test": "stripes test nightmare",
    "lint": "eslint src"
  },
  "devDependencies": {
    "@folio/eslint-config-stripes": "^3.2.1",
    "@folio/stripes": "^1.0.0",
    "@folio/stripes-cli": "^1.5.0",
    "babel-eslint": "^9.0.0",
    "eslint": "^5.6.0"
  },
  "dependencies": {
    "prop-types": "^15.6.0",
    "react-router-dom": "^4.1.1",
    "@folio/stripes-components": "^4.2.0",
    "redux-form": "^7.4.2"
  },
  "peerDependencies": {
    "@folio/stripes-core": "^2.14.0",
    "react": "*"
  },
  "optionalDependencies": {
    "@folio/plugin-find-vendor": "^1.0.0" 
  },
  "stripes": {
    "type": "app",
    "displayName": "eUsage",
    "route": "/ermusage",
    "home": "/ermusage",
    "hasSettings": true,
    "queryResource": "query",
    "okapiInterfaces": {
      "erm-usage": "0.0.1",
      "vendor": "1.0"
    },
    "permissionSets": [
      {
        "permissionName": "module.erm-usage.enabled",
        "displayName": "UI: erm-usage module is enabled",
        "visible": true
      },
      {
        "permissionName": "settings.erm-usage.enabled",
        "displayName": "Settings (erm-usage): display list of settings pages",
        "subPermissions": [
          "settings.enabled"
        ],
        "visible": true
      },
      {
        "permissionName": "ui-erm-usage.view ",
        "displayName": "eUsage: Can view usage data providers and COUNTER reports",
        "description": "Can view usage data providers and COUNTER reports",
        "subPermissions": [
          "module.erm-usage.enabled",
          "ermusage.view"
        ],
	      "visible": true
      }
    ]
  }
}
