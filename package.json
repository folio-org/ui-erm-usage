{
  "name": "@folio/erm-usage",
  "version": "1.0.1",
  "description": "eUsage Module",
  "main": "src/index.js",
  "repository": "folio-org/ui-erm-usage",
  "license": "Apache-2.0",
  "engines": {
    "node": ">=6.0.0"
  },
  "scripts": {
    "start": "stripes serve",
    "build": "stripes build --output ./output",
    "test": "echo 'No unit tests implemented'",
    "test-int": "stripes test nightmare --run udp-crud --show",
    "test-udp-crud": "stripes test nightmare --run udp-crud --show",
    "lint": "eslint src"
  },
  "devDependencies": {
    "@folio/eslint-config-stripes": "^3.2.1",
    "@folio/stripes": "^1.1.0",
    "@folio/stripes-cli": "^1.7.0",
    "babel-eslint": "^9.0.0",
    "eslint": "^5.6.0"
  },
  "dependencies": {
    "file-saver": "^2.0.0",
    "lodash": "^4.17.11",
    "prop-types": "^15.6.0",
    "react-intl": "^2.7.1",
    "react-router-dom": "^4.1.1",
    "redux-form": "^7.4.2"
  },
  "peerDependencies": {
    "@folio/stripes": "^1.1.0",
    "react": "*"
  },
  "stripes": {
    "type": "app",
    "displayName": "ui-erm-usage.meta.title",
    "route": "/eusage",
    "home": "/eusage?filters=harvestingStatus.Active",
    "hasSettings": true,
    "queryResource": "query",
    "okapiInterfaces": {
      "usage-data-providers": "1.0",
      "aggregator-settings": "1.0",
      "counter-reports": "1.0",
      "vendor": "1.0"
    },
    "permissionSets": [
      {
        "permissionName": "module.erm-usage.enabled",
        "displayName": "UI: erm-usage module is enabled",
        "visible": true
      },
      {
        "permissionName": "settings.erm-usage.enabled",
        "displayName": "Settings (erm-usage): display list of settings pages",
        "subPermissions": [
          "settings.enabled"
        ],
        "visible": true
      },
      {
        "permissionName": "ui-erm-usage.view ",
        "displayName": "eUsage: Can view usage data providers and COUNTER reports",
        "description": "Can view usage data providers and COUNTER reports",
        "subPermissions": [
          "module.erm-usage.enabled",
          "ermusage.view"
        ],
        "visible": true
      }
    ],
    "icons": [
      {
        "name": "app",
        "alt": "ERM usage statistics",
        "title": "eUsage"
      }
    ]
  }
}
